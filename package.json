    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tslib": "^2.6.2",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "url": "^0.11.3",
    "util": "^0.12.5"
  },
  "dependencies": {
    "axios": "^1.6.6"
  },
  "keywords": [
